<header>
  <div class="container header">
    <div id="logo">
      <img src="/img/logo.png" />
    </div>
    <div class="busca">
      <form>
        <input type="search" class="search" placeholder="buscar usuarios" />
      </form>
    </div>
    <nav>
      <a href="/publicar">
        <img
          src="/img/icons/plus.svg"
          alt="Adicionar Publicação"
          title="Adicionar Publicação"
        />
      </a>
      <a href="/home">
        <img src="/img/icons/home.svg" />
      </a>
      <a href="">
        <div class="avatar-content">
          <img class="avatar" src="/uploads/<%=user.avatar%>" />
          <b><%=user.username%></b>
        </div>
      </a>
    </nav>
  </div>
</header>
